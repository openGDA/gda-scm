<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

        <bean id="box2_m9pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.M9P" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_m9pitch" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_m9pitch_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_m9roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.M9R" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_m9roll" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_m9roll_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_m10pitch_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.M10P" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_m10pitch" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_m10pitch_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_m10roll_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.M10R" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_m10roll" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_m10roll_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_m5z_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.M5Z" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_m5z" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_m5z_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_xexit_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.XEXIT" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_xexit" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_xexit_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_yexit_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.YEXIT" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_yexit" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_yexit_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_thetaexit_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.THETAEXIT" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_thetaexit" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_thetaexit_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2_phiexit_motor" class="gda.device.motor.EpicsMotor">
		<property name="deviceName" value="BOX2.PHIEXIT" />
                <property name="local" value="true" />
        </bean>
        <bean id="box2_phiexit" class="gda.device.scannable.ScannableMotor">
                <property name="motor" ref="box2_phiexit_motor" />
                <property name="local" value="true" />
        </bean>

        <bean id="box2" class="gda.device.scannable.scannablegroup.ScannableGroup">
                <property name="groupMembers">
                        <list>
                                <ref bean="box2_m9pitch" />
                                <ref bean="box2_m9roll" />
                                <ref bean="box2_m10pitch" />
                                <ref bean="box2_m10roll" />
                                <ref bean="box2_m5z" />
                                <ref bean="box2_xexit" />
                                <ref bean="box2_yexit" />
                                <ref bean="box2_thetaexit" />
                                <ref bean="box2_phiexit" />
                        </list>
                </property>
                <property name="local" value="true" />
        </bean>
</beans>
