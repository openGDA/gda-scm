    <bean class="gda.device.scannable.ScannableMotor" id="s2_yplus">
        <property name="motor" ref="s2_yplus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s2_xminus">
        <property name="motor" ref="s2_xminus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s2_ycentre">
        <property name="motor" ref="s2_ycentre_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
</beans>
