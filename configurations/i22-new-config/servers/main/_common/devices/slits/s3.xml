<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba" xmlns:tx="http://www.springframework.org/schema/tx" xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd                         http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd                                 http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd                                         http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd                                                 http://www.diamond.ac.uk/schema/gda/corba http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.0.xsd">
    <bean class="gda.device.scannable.ScannableMotor" id="s3_xgap">
        <property name="motor" ref="s3_xgap_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_ygap">
        <property name="motor" ref="s3_ygap_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_xcentre">
        <property name="motor" ref="s3_xcentre_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_xminus">
        <property name="motor" ref="s3_xminus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_xplus">
        <property name="motor" ref="s3_xplus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_ycentre">
        <property name="motor" ref="s3_ycentre_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.scannablegroup.ScannableGroup" id="s3">
        <property name="groupMembers">
            <list>
                <ref bean="s3_xcentre"/>
                <ref bean="s3_xgap"/>
                <ref bean="s3_ycentre"/>
                <ref bean="s3_ygap"/>
                <ref bean="s3_xplus"/>
                <ref bean="s3_xminus"/>
                <ref bean="s3_yplus"/>
                <ref bean="s3_yminus"/>
            </list>
        </property>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_yminus">
        <property name="motor" ref="s3_yminus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s3_yplus">
        <property name="motor" ref="s3_yplus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
</beans>
