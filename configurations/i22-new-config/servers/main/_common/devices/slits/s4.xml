<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:corba="http://www.diamond.ac.uk/schema/gda/corba" xmlns:tx="http://www.springframework.org/schema/tx" xmlns:util="http://www.springframework.org/schema/util" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd                         http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd                                 http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd                                         http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd                                                 http://www.diamond.ac.uk/schema/gda/corba http://www.diamond.ac.uk/schema/gda/corba/gda-corba-1.0.xsd">
    <bean class="gda.device.scannable.ScannableMotor" id="s4_xcentre">
        <property name="motor" ref="s4_xcentre_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_ycentre">
        <property name="motor" ref="s4_ycentre_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_xgap">
        <property name="motor" ref="s4_xgap_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_xplus">
        <property name="motor" ref="s4_xplus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_xminus">
        <property name="motor" ref="s4_xminus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.scannablegroup.ScannableGroup" id="s4">
        <property name="groupMembers">
            <list>
                <ref bean="s4_xcentre"/>
                <ref bean="s4_xgap"/>
                <ref bean="s4_ycentre"/>
                <ref bean="s4_ygap"/>
                <ref bean="s4_xplus"/>
                <ref bean="s4_xminus"/>
                <ref bean="s4_yplus"/>
                <ref bean="s4_yminus"/>
            </list>
        </property>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_yplus">
        <property name="motor" ref="s4_yplus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_yminus">
        <property name="motor" ref="s4_yminus_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
    <bean class="gda.device.scannable.ScannableMotor" id="s4_ygap">
        <property name="motor" ref="s4_ygap_motor"/>
        <property name="local" value="true"/>
        <property name="protectionLevel" value="3"/>
    </bean>
</beans>
