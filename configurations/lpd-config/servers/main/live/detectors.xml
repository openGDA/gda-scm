<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="lpdDetector" class="gda.device.detector.LpdDetector">
<!--		<property name="femClient" value="lpdFemClient" />-->
		<property name="xmodules" value="2" />
		<property name="ymodules" value="2" />
		<property name="lpdModuleNames">
			<list>
				<value>lpdModule1</value>
				<value>lpdModule2</value>
				<value>lpdModule3</value>
				<value>lpdModule4</value>
			</list>
		</property>
	</bean>
</beans>