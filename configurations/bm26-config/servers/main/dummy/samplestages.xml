<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="LinkamT95" class="gda.device.temperature.DummyTemp">
		<property name="configureAtStartup" value="false" />
		<property name="upperTemp" value="600" />
		<property name="lowerTemp" value="-195" />
	</bean>

	<bean id="Eurotherm2400" class="gda.device.temperature.DummyTemp">
		<property name="configureAtStartup" value="false" />
		<property name="accuracy" value="2" />
		<property name="polltime" value="5000" />
	</bean>

</beans>